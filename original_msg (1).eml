<!DOCTYPE html>
<html lang=3D"pt-BR">
<head>
  <meta charset=3D"UTF-8" />
  <meta name=3D"viewport" content=3D"width=3Ddevice-width, initial-scale=3D=
1.0"/>
  <title>Sorteador Roxo</title>
  <style>
    body {
      margin: 0;
      padding: 0;
      background-color: #f4f0fb;
      font-family: 'Segoe UI', sans-serif;
      color: #2e2e2e;
      display: flex;
      align-items: center;
      justify-content: center;
      min-height: 100vh;
    }

    .wrapper {
      width: 90%;
      max-width: 700px;
      background: #fff;
      border-radius: 20px;
      box-shadow: 0 10px 30px rgba(126, 91, 239, 0.15);
      padding: 40px;
      text-align: center;
    }

    h1 {
      color: #7e5bef;
      font-size: 32px;
      margin-bottom: 10px;
    }

    .subtitle {
      color: #6b5e85;
      font-size: 16px;
      margin-bottom: 20px;
    }

    .item-box {
      margin-bottom: 20px;
    }

    .item-box input {
      width: 100%;
      padding: 14px;
      font-size: 16px;
      border-radius: 12px;
      border: 1px solid #d8cdf6;
      background-color: #faf8ff;
      color: #2e2e2e;
      outline: none;
      transition: border-color 0.3s ease, box-shadow 0.3s ease;
    }

    .item-box input:focus {
      border-color: #7e5bef;
      box-shadow: 0 0 0 4px rgba(126, 91, 239, 0.15);
    }

    .list-box {
      margin-top: 20px;
      text-align: left;
      max-height: 200px;
      overflow-y: auto;
      border: 1px solid #d6c9f6;
      border-radius: 12px;
      padding: 10px;
      background-color: #faf8ff;
    }

    .chip {
      background: #e4d9fd;
      color: #5e3dbb;
      padding: 6px 12px;
      border-radius: 999px;
      font-size: 14px;
      margin-right: 8px;
      margin-top: 8px;
      display: inline-block;
    }

    .range-box {
      margin-top: 30px;
      display: flex;
      justify-content: center;
      gap: 16px;
    }

    .range-box input {
      width: 100px;
      padding: 10px;
      border-radius: 10px;
      border: 1px solid #d3cce9;
      background: #faf8ff;
      color: #2e2e2e;
      text-align: center;
      font-size: 16px;
    }

    .draw-btn {
      margin-top: 40px;
      padding: 16px 36px;
      background-color: #7e5bef;
      color: white;
      font-size: 18px;
      font-weight: bold;
      border: none;
      border-radius: 14px;
      cursor: pointer;
      transition: background 0.3s ease, transform 0.1s ease;
    }

    .draw-btn:hover {
      background-color: #5e3dbb;
      transform: scale(1.03);
    }

    .result-box {
      margin-top: 40px;
      background: #faf8ff;
      padding: 24px;
      border-radius: 16px;
      border: 2px solid #e0d2fd;
    }

    .result-label {
      color: #5e3dbb;
      font-weight: 600;
      font-size: 16px;
    }

    .result-value {
      font-size: 28px;
      font-weight: bold;
      color: #7e5bef;
      margin-top: 8px;
    }

    footer {
      margin-top: 40px;
      font-size: 13px;
      color: #999;
    }
  </style>
</head>
<body>
  <div class=3D"wrapper">
    <h1>=F0=9F=8E=B2 Sorteador Roxo</h1>
    <div class=3D"subtitle">Adicione nomes ou use um intervalo num=C3=A9ric=
o</div>

    <div class=3D"item-box">
      <input type=3D"text" id=3D"itemName" placeholder=3D"Nome do item sort=
eado
(ex: Brinde, Pr=C3=AAmio 1)" />
    </div>

    <div class=3D"list-box" id=3D"nameList">
      <!-- Nomes adicionados aparecer=C3=A3o aqui -->
    </div>

    <div class=3D"item-box">
      <input type=3D"text" id=3D"nameInput" placeholder=3D"Digite um nome" =
/>
      <button id=3D"addNameBtn" class=3D"draw-btn" style=3D"margin-top:
10px;">Adicionar
Nome</button>
    </div>

    <div class=3D"range-box">
      <input type=3D"number" id=3D"min" placeholder=3D"M=C3=ADnimo" value=
=3D"1">
      <input type=3D"number" id=3D"max" placeholder=3D"M=C3=A1ximo" value=
=3D"10">
    </div>

    <button class=3D"draw-btn" id=3D"drawBtn">Sortear Agora</button>

    <div class=3D"result-box" id=3D"resultBox" style=3D"display: none;">
      <div class=3D"result-label">Resultado:</div>
      <div class=3D"result-value" id=3D"resultWinner">...</div>
    </div>

    <footer>
      Feito com =F0=9F=92=9C e sorte! =E2=80=94 Digite nomes ou use interva=
lo de n=C3=BAmeros.
    </footer>
  </div>

  <script>
    const nameInput =3D document.getElementById('nameInput');
    const addNameBtn =3D document.getElementById('addNameBtn');
    const drawBtn =3D document.getElementById('drawBtn');
    const nameList =3D document.getElementById('nameList');
    const resultBox =3D document.getElementById('resultBox');
    const resultWinner =3D document.getElementById('resultWinner');
    const minInput =3D document.getElementById('min');
    const maxInput =3D document.getElementById('max');
    const itemNameInput =3D document.getElementById('itemName');

    // Adicionar nome =C3=A0 lista
    addNameBtn.addEventListener('click', () =3D> {
      const name =3D nameInput.value.trim();
      if (name) {
        const chip =3D document.createElement('div');
        chip.className =3D 'chip';
        chip.textContent =3D name;
        nameList.appendChild(chip);
        nameInput.value =3D '';
      }
    });

    // Sortear
    drawBtn.addEventListener('click', () =3D> {
      const chips =3D nameList.querySelectorAll('.chip');
      const itemName =3D itemNameInput.value.trim() || 'Item';

      let winner =3D '';

      if (chips.length > 0) {
        const names =3D Array.from(chips).map(chip =3D> chip.textContent);
        const randomIndex =3D Math.floor(Math.random() * names.length);
        winner =3D names[randomIndex];
      } else {
        const min =3D parseInt(minInput.value, 10);
        const max =3D parseInt(maxInput.value, 10);
        if (isNaN(min) || isNaN(max) || max < min) {
          alert('Digite um intervalo v=C3=A1lido.');
          return;
        }
        const randomNum =3D Math.floor(Math.random() * (max - min + 1)) + m=
in;
        winner =3D randomNum;
      }

      resultWinner.textContent =3D `${itemName}: ${winner}`;
      resultBox.style.display =3D 'block';
    });
  </script>
</body>
</html>

